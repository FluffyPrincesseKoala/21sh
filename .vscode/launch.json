	"version": "0.2.0",
	"configurations": [
		{
			"name": "(gdb) Attach",
			"type": "cppdbg",
			"request": "attach",
			"program": "enter program name, for example ${workspaceFolder}/a.out",
			"processId": "${command:pickProcess}",
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			]
		},
		{
			"name": "(gdb) Lancer",
			"type": "cppdbg",
			"request": "launch",
			"program": "${cwd}/21sh",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
				   "description": "Skip vscode-remote://wsl+ubuntu/build/glibc-YYA7BZ/glibc-2.31/sysdeps/unix/sysv/linux/raise.c files",
				   "text": "-interpreter-exec console \"skip -gfi vscode-remote://wsl+ubuntu/build/glibc-YYA7BZ/glibc-2.31/sysdeps/unix/sysv/linux/raise.c/**/*\""
				}
			 ]
		},
	]
}